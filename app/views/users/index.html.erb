<h1>Listing users</h1>

  
    <% @users.each do |user| %>
    
        <h3><%= user.firstName + " " + user.lastName %></h3>
        <%= user.email %>
        <div><%= image_tag user.picture.url(:medium), class: "img-rounded" %></div>
        
        <%= link_to 'Show', user %>
        <%= link_to 'Edit', edit_user_path(user) %>
       
   
    <% end %>
 

<br>

<%= link_to 'New User', new_user_path %>
